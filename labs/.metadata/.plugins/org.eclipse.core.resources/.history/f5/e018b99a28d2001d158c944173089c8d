package com.clock.components;

import java.awt.Graphics;

import com.clock.util.Vector;

public class DiagonalLine extends Component {

	
	private Vector start;
	private Vector end;
	private double angle;
	
	public DiagonalLine(Vector start, double angle, double length) {
		this.start = start;
		this.end = Vector.getEndFromAngle(k, angle, angle)
	}

	@Override
	public void update() {
		
	}

	@Override
	public void paint(Graphics g) {
		
		g.drawLine(pos.getScaledX(), pos.getScaledY(), );
	}
}
